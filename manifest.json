{
    "manifest_version": 3,
    "name": "download",
    "version": "1.0",
    "description": "this is a download ext",

    "permissions": [
        "downloads","storage","tabs"
    ],

    "background": {
        "scripts":["background.js"]
    },
    "action":{
        "default_popup":"popup.html"
    },
    "commands": {
    "_execute_action": {
        "suggested_key": {
        "default": "Ctrl+Shift+Q"
        },
        "description": "Open popup"
    }
    }

}