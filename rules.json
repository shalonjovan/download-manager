{
    "rules": [
        {
            "name": "CTF related files from picoCTF and CTFlearn",
            "conditions": {
                "domains": [
                    "picoctf.org",
                    "ctflearn.com"
                ]
            },
            "path":"CTF"
        },
        {
            "name": "videos",
            "conditions": {
                "extensions": ["jpg","mp4", "mov", "wmv", "flv", "avi", "mkv"]
            },
            "path":"Videos"
        }
    ],
    "default": {
        "path": "Downloads"
    }
}

